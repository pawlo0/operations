wb = xlsx_package.workbook
wb.add_worksheet(name: "nterventions") do |sheet|
  columns_arr = []
  not_export = ["id", "created_at", "updated_at"]
  @interventions.column_names.each do |col_name|
    columns_arr << col_name unless not_export.include?(col_name)
  end
  sheet.add_row columns_arr
  @interventions.each do |element|
    line = sheet.add_row
    columns_arr.each do |col_name|
      line.add_cell element.send(col_name)
    end
  end
end