<div class = "span9">

<h2>Tipos de intervenção</h2>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Observações</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intervention_types.each do |intervention_type| %>
      <tr>
        <td><%= intervention_type.name %></td>
        <td><%= intervention_type.obs %></td>
        <td><%= link_to 'Edit', edit_intervention_type_path(intervention_type), :class => 'btn btn-default' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div><!-- span9 -->

<div class="span3">
  <%= render 'side' %>
</div>
<br>
